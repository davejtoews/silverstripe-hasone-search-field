{
    "name": "davejtoews/silverstripe-hasone-search-field",
    "description": "Edit a has_one relationship using a GridField, triggered by clicking a button.",
    "license": "BSD-2-Clause",
    "type": "silverstripe-vendormodule",
    "require": {
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/framework": "^4.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "authors": [{
        "name": "David Toews",
        "email": "davejtoews@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "DaveJToews\\HasOneField\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
